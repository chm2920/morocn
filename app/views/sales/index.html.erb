							arr.push('</a>');
						}
						if(info[t].msn){
							arr.push('<a target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=' + info[t].qq + '&amp;site=qq&amp;menu=yes">');
							arr.push('<img border="0" src="http://wpa.qq.com/pa?p=2:' + info[t].qq + ':51" alt="点击这里给我发消息" title="点击这里给我发消息">');
