						arr.push('					<td>' + info[t].phone + '</td>');
						arr.push('				</tr>');
						arr.push('				<tr>');
						arr.push('					<td class="sale-label">在线客服：</td>');
						arr.push('					<td>');
						if(info[t].qq){
							arr.push('<a target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=' + info[t].qq + '&amp;site=qq&amp;menu=yes">');
