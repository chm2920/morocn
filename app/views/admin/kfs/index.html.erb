<div class="nav">
	<div class="list-opts">
		<div class="nav-links">
			<a href="/admin/kfs">在线客服</a>
			<a href="/admin/kfs/new">添加客服</a>
		</div>
	</div>
</div>
<div class="main">
<table class="stdtable edtable">
	<thead>
		<tr>
			<th width="10%">编号</th>
			<th width="15%">类型</th>
			<th width="15%">标题</th>
			<th>帐号</th>
			<th width="30%">管理</th>
		</tr>
	</thead>
	<tbody>
	<%
		if @kfs.length>0
			@kfs.each do |kf|
	%>
		<tr>
			<td><%= kf.id %></td>
			<td><%= kf.stype %></td>
			<td><%= kf.title %></td>
			<td><%= kf.account %></td>
			<td>
				<a href="/admin/kfs/<%= kf.id %>/edit">修改</a>|<%= link_to "删除", {:action => "destroy", :id => kf}, :confirm => "确定删除？", :method => "delete" %>
			</td>
		</tr>
	<%
			end
		else
	%>
		<tr>
			<td colspan="5" style="text-align:left;">&nbsp;还没有内容。</td>
		</tr>
	<%
		end
	%>
	</tbody>
</table>
</div>